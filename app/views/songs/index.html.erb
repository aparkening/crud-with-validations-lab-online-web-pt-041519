<h1>All Songs</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Artist</th>
    </tr>
  </thead>
  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= link_to(song.title, song_path(song)) %></td>
        <%= content_tag(:td, song.artist_name) %>
      </tr>
    <% end %>
  </tbody>
</table>